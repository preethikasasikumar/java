public class Main {
    public static void main(String[] args) {
       for(int i = 500;i<=2500;i++){
           int num = i;
           int sum = 0;
           int temp = num;
           while(num>0){
               int rem = num % 10;
               sum = sum + rem;
               num= num/10;
               
               
           }
           if(temp%sum==0){
               System.out.println(temp);
           }
       }
    }
}
