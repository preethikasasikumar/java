import java.util.Scanner;

class Add{
    private int a, b, c;
    public void sum(){
        Scanner scanner = new Scanner(System.in);
            a = scanner.nextInt();
            b = scanner.nextInt();
            c = a+ b;
            System.out.println("Sum :" +" " + c);
        }
    }



class Main{
    
	public static void main(String[] args) {
		Add addition = new Add();
		addition.sum();
	}
    }


