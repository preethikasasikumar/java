// for  loops and for each loop

public class Main
{
	public static void main(String[] args) {
		int arr[] = {1,2,3,4,5};
		int n = arr.length;
		for(int i = 1;i<=5;i++){
		    System.out.println(i);
		}
		for(int i = n- 1;i>=0;i--){
		    System.out.println(i);
		}
		for(int i : arr){
		    System.out.println(i);
		}
	}
}
