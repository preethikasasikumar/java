public class Main {
    public static void main(String[] args) {
        int x = 9;
        int n = 5;
        int sum = 0;

        // Loop to generate numbers and calculate sum
        for (int i = 1; i <= n; i++) {
            int num = 0; // Variable to store the number formed by repeating '9'

            // Generate the number by repeating '9' i times
            for (int j = 1; j <= i; j++) {
                num = num * 10 + x; // Shift the digits left and add '9'
            }

            sum += num; // Add the number to the sum
        }

        // Print the sum
        System.out.println("Sum: " + sum);
    }
}
