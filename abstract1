abstract class Circle{
    static double area, circum,r;
    static void get (int x){
         r = x;
    }
    static void calculate_area(){
        area =3.14 * r * r;
        System.out.println("Area = "+area);
    }
    static void calculate_circum(){
        circum = 2 * 3.14 * r;
        System.out.println("Circumferenc = " +circum);
    }
    static  public void main(String []args){
        get(89);
        calculate_area();
        calculate_circum();
    }
}
