abstract class A{
    abstract void display();
    
    void show(){
        System.out.println("inside A show");
    } 
    abstract class B{
    abstract public void call();
    
    void print(){
        System.out.println("inside B print");
    }
    
}

    
}
class X extends A{
    public void display(){
        System.out.println("abstract from A");
    }
    public void call(){
        System.out.println("Abstract from B");
    }
    
    
}

class Main{
    public static void main(String []args){
       
        X x = new X();
        x.display();
        x.call();
        
    
    }
}
